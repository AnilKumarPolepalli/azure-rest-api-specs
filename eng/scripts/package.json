{
  "devDependencies": {
    "eslint": "^9.30.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "lint": "eslint",
    "prettier": "prettier \"**/*.js\" --check",
    "prettier:debug": "prettier \"**/*.js\" --check ---log-level debug",
    "prettier:write": "prettier \"**/*.js\" --write",
    "test": "vitest",
    "test:ci": "vitest run --coverage --reporter=verbose"
  },
  "type": "module"
}
