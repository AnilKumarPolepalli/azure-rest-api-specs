{
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "lint": "eslint",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "format:check:ci": "prettier . --check --log-level debug",
    "test": "vitest",
    "test:ci": "vitest run --coverage --reporter=verbose"
  },
  "type": "module"
}
